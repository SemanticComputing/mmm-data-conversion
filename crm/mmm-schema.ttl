@prefix : <http://ldf.fi/mmm/schema/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix frbroo: <http://erlangen-crm.org/efrbroo/> .


:entry a rdf:Property;
  rdfs:label "Source entry in SDBM"@en .

:manuscript_record a rdf:Property;
  rdfs:label "Source manuscript record in SDBM"@en ;
  rdfs:domain frbroo:F4_Manifestation_Singleton .

:order a rdf:Property;
  rdfs:label "Ordering number"@en ;
  rdfs:comment "Used, e.g., for ordering of provenance information"@en ;
  rdfs:domain rdf:Statement .

:Width a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Width"@en .

:Height a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Height"@en .

:Folios a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Folio count"@en .

:Lines a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Line count"@en .

:Columns a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Column count"@en .

:Miniatures a rdfs:Class ;
  rdfs:subClassOf crm:E54_Dimension ;
  rdfs:label "Miniature count"@en .

:Observation a rdfs:Class ;
  rdfs:subClassOf crm:E5_Event ;
  rdfs:label "Direct or indirect observation of a manuscript"@en .

:selling_agent a rdf:Property;
  rdfs:subPropertyOf crm:P14_carried_out_by ;
  rdfs:label "Selling agent"@en .

:observed_manuscript a rdf:Property;
  rdfs:subPropertyOf crm:P12_occured_in_the_presence_of ;
  rdfs:label "Observed manuscript"@en .
